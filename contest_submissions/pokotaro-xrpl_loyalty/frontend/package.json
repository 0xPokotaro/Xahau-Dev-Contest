{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ts-node": "dotenv -e .env.local ts-node",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset --force",
    "db:seed": "dotenv -e .env.local -- prisma db seed"
